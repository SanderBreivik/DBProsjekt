         <content>
            <ListView fx:id="groups" prefHeight="200.0" prefWidth="305.0" />
         </content>
      </ScrollPane>
      <ScrollPane layoutX="20.0" layoutY="333.0" prefHeight="200.0" prefWidth="311.0">
         <content>
            <ListView fx:id="exercises" prefHeight="200.0" prefWidth="297.0" />
