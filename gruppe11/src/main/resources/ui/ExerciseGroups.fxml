<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="600.0" prefWidth="710.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.ExerciseGroupController">
   <children>
      <Button fx:id="back" minWidth="66.0" mnemonicParsing="false" onAction="#back" prefHeight="26.0" prefWidth="96.0" text="Tilbake">
         <VBox.margin>
            <Insets bottom="50.0" left="5.0" right="5.0" top="5.0" />
         </VBox.margin>
      </Button>
      <HBox alignment="CENTER">
         <children>
            <VBox alignment="CENTER">
               <children>
                  <TextField fx:id="name" focusTraversable="false" prefHeight="26.0" prefWidth="203.0" promptText="Navn på ny øvelsesgruppe">
                     <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </VBox.margin>
                  </TextField>
                  <Button fx:id="add" mnemonicParsing="false" onAction="#toAdd" prefHeight="26.0" prefWidth="140.0" text="Ny øvelsesgruppe">
                     <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </VBox.margin>
                  </Button>
                  <Label prefHeight="18.0" prefWidth="131.0" text="Alle øvelsesgrupper: " />
                  <ListView fx:id="groups" prefHeight="200.0" prefWidth="250.0" />
                  <Button fx:id="show" mnemonicParsing="false" onAction="#toShow" text="Vis øvelser i gruppe">
                     <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </VBox.margin>
                  </Button>
               </children>
            </VBox>
         </children>
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </VBox.margin>
      </HBox>
      <HBox alignment="CENTER">
         <children>
            <VBox alignment="CENTER">
               <children>
                  <Label prefHeight="18.0" prefWidth="107.0" text="Øvelser i gruppe:" />
                  <ListView fx:id="exercises" prefHeight="200.0" prefWidth="250.0" />
               </children>
            </VBox>
            <VBox alignment="CENTER">
               <children>
                  <Button fx:id="one" mnemonicParsing="false" onAction="#removeEx" prefHeight="26.0" prefWidth="140.0" text="Fjern øvelse -&gt;">
                     <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="two" mnemonicParsing="false" onAction="#addEx" prefHeight="26.0" prefWidth="140.0" text="&lt;- Legg til øvelse">
                     <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </VBox.margin>
                  </Button>
               </children>
               <HBox.margin>
                  <Insets top="5.0" />
               </HBox.margin>
            </VBox>
            <VBox alignment="CENTER">
               <children>
                  <Label prefHeight="18.0" prefWidth="79.0" text="Alle øvelser:" />
                  <ListView fx:id="allExercises" prefHeight="200.0" prefWidth="250.0" />
               </children>
            </VBox>
         </children>
         <VBox.margin>
            <Insets bottom="20.0" left="5.0" right="5.0" top="5.0" />
         </VBox.margin>
      </HBox>
   </children>
</VBox>
