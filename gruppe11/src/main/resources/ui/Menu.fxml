<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<HBox prefHeight="600.0" prefWidth="710.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.MenuController">
   <children>
      <HBox>
         <children>
            <Button fx:id="exitBtn" mnemonicParsing="false" onAction="#exit" text="Lukk">
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
            </Button>
            <Region prefHeight="200.0" prefWidth="180.0" />
            <VBox alignment="CENTER">
               <children>
                  <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="102.0" prefWidth="245.0" text="Treningsappen">
                     <font>
                        <Font size="31.0" />
                     </font>
                  </Label>
                  <Button fx:id="number" minWidth="185.0" mnemonicParsing="false" onAction="#toOverview" text="Antall treningsøkter">
                     <VBox.margin>
                        <Insets bottom="5.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="add" minWidth="185.0" mnemonicParsing="false" onAction="#toAdd" text="Legg til treningsøkt">
                     <VBox.margin>
                        <Insets bottom="5.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="logs" minWidth="185.0" mnemonicParsing="false" onAction="#toResult" text="Resultatlogg">
                     <VBox.margin>
                        <Insets bottom="5.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="exercise" minWidth="185.0" mnemonicParsing="false" onAction="#toExercise" text="Øvelser">
                     <VBox.margin>
                        <Insets bottom="5.0" />
                     </VBox.margin>
                  </Button>
               </children>
            </VBox>
            <Region prefHeight="200.0" prefWidth="200.0" />
         </children>
      </HBox>
   </children>
</HBox>
